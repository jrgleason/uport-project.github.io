{
  "name": "uport-site-generator",
  "description": "Uport documentation generator",
  "version": "2.2.1",
  "author": "team@uport.me",
  "dependencies": {
    "fancy-log": "^1.3.2",
    "gatsby": "^1.9.223",
    "gatsby-link": "^1.6.38",
    "gatsby-plugin-catch-links": "^1.0.19",
    "gatsby-plugin-feed": "^1.3.19",
    "gatsby-plugin-google-analytics": "^1.0.20",
    "gatsby-plugin-google-fonts": "0.0.4",
    "gatsby-plugin-manifest": "^1.0.15",
    "gatsby-plugin-nprogress": "^1.0.13",
    "gatsby-plugin-offline": "^1.0.15",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-segment-js": "^2.0.1",
    "gatsby-plugin-sitemap": "^1.2.14",
    "gatsby-plugin-styled-components": "^2.0.9",
    "gatsby-plugin-twitter": "^1.0.18",
    "gatsby-remark-autolink-headers": "^1.4.13",
    "gatsby-remark-copy-images": "^0.2.1",
    "gatsby-remark-copy-linked-files": "^1.5.30",
    "gatsby-remark-images": "^1.5.55",
    "gatsby-remark-prismjs": "^2.0.4",
    "gatsby-remark-responsive-iframe": "^1.4.18",
    "gatsby-source-filesystem": "^1.5.26",
    "gatsby-transformer-remark": "^1.7.34",
    "gulp": "^4.0.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-watch": "^5.0.0",
    "lodash": "^4.17.5",
    "lodash-webpack-plugin": "^0.11.4",
    "npm": "^6.0.1",
    "prism-themes": "^1.0.1",
    "prismjs": "^1.15.0",
    "qrcode.react": "^0.8.0",
    "react": "^16.4.1",
    "react-burger-menu": "^2.4.2",
    "react-disqus-comments": "^1.0.3",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.1.3",
    "react-icons": "^2.2.7",
    "react-share": "^2.2.0",
    "react-twitter-widgets": "^1.4.0",
    "rehype-react": "^3.0.2",
    "styled-components": "^3.3.2",
    "uport-connect": "^0.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-pug": "^6.0.0",
    "babel-preset-env": "^1.6.1",
    "cli-glob": "^0.1.0",
    "es-collections": "^0.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "gh-pages": "^1.1.0",
    "gulp-concat": "^2.6.0",
    "jsdoc": "3.5.5",
    "minimist": "^1.2.0",
    "prettier": "^1.11.1",
    "remark-cli": "^5.0.0",
    "remark-preset-lint-recommended": "^3.0.1",
    "stylefmt": "^6.0.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "write-good": "^0.13.0"
  },
  "keywords": [
    "uport",
    "ethereum",
    "ipfs",
    "identity"
  ],
  "license": "Apache-2.0",
  "main": "n/a",
  "scripts": {
    "clean:repos": "rm -rf repos/*",
    "watch:repos": "./node_modules/.bin/gulp watch:repos",
    "fetch": "./node_modules/.bin/gulp git:clone",
    "copy:markdown": "./node_modules/.bin/gulp copy-markdown",
    "install:plugins": "cd ./plugins/gatsby-remark-copy-linked-files && npm install && cd ../gatsby-remark-catch-relative-markdown-links && npm install && cd ../../",
    "update:remote:docs": "npm run clean:repos && npm run fetch && npm run copy:markdown",
    "setup": "npm run install:plugins && npm run clean && npm run update:remote:docs",
    "develop": "./node_modules/.bin/gatsby develop",
    "dev": "npm run develop",
    "serve": "./node_modules/.bin/gatsby serve & ngrok http 9000 && kill $!",
    "build": "./node_modules/.bin/gatsby build",
    "build:pp": "./node_modules/.bin/gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build && gh-pages -b master -d public",
    "clean": "rm -rf public .cache node_modules/gh-pages/.cache",
    "lint:js": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public,static .",
    "lint:md": "./node_modules/.bin/remark content/public/",
    "write-good": "./node_modules/.bin/write-good $(glob 'content/public/**/*.md')",
    "format:js": "./node_modules/.bin/prettier '**/*.{js,jsx}' --write"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
